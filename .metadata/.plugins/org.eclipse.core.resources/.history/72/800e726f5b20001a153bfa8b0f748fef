package apicacao;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		//adicionando elementos na lista
		list.add("Ramon");
		list.add("Maria");
		
		for (String x: list) {
			System.out.println(x);
		}

	}

}
